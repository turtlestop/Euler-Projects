    "# euler 16\n",
    "\n",
    "# 2**15 = 32768\n",
    "# the sum of its digits is 26\n",
    "\n",
    "# What's the sum of the digits of the number 2**1000?\n",
    "\n",
    "digitsum = 0\n",
    "bignum = 2**1000\n",
    "for digit in str(bignum):\n",
    "    digitsum += int(digit)\n",
    "print digitsum"
